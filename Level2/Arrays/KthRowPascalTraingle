# http://www.geeksforgeeks.org/pascal-triangle/
class Solution:

  @staticmethod
  def solution(N):
        n = N
        res = []
        row = []
        for line in range(1, n+1):
            c = 1
            row = []
            for i in range(1, line+1):
                row.append(c)
                c = c * (line - i)/i
            res.append(row)

        return res

a = 3
res = Solution.solution(a)
print(res,end= " ")
